{
  "name": "health-ui",
  "version": "0.0.2",
  "description": "health - 丰富、多样的移动端 Vue 组件库",
  "author": "haotengfei",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "files": [
    "es",
    "lib",
    "packages",
    "types",
    "vetur"
  ],
  "scripts": {
    "bootstrap": "npm i || yarn",
    "dev": "npm run build:entry && webpack-dev-server --config build/webpack.dev.js",
    "lint": "eslint ./packages --ext .js,.vue && stylelint \"packages/*.css\"",
    "lint:fix": "eslint ./packages --fix --ext .js,.vue && stylelint \"packages/*.css\" ",
    "build:entry": "node build/build-entry.js",
    "build:components": "node build/build-components.js --color",
    "build:health": "cross-env NODE_ENV=production webpack --color --config build/webpack.build.js && cross-env NODE_ENV=production webpack -p --color --config build/webpack.build.js",
    "build:style": "gulp --gulpfile ./build/build-style.js",
    "build:style-entry": "node build/build-style-entry.js",
    "build:lib": "node build/build-lib.js",
    "build": "rimraf docs/dist && cross-env NODE_ENV=production webpack --config build/webpack.doc.js",
    "release": "npm run build:lib && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [],
    "*.{vue,css}": []
  },
  "keywords": [
    "health",
    "vue",
    "component"
  ],
  "dependencies": {
    "fs-extra": "^8.1.0",
    "vue-clipboard2": "^0.3.1"
  },
  "peerDependencies": {
    "vue": ">= 2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-object-assign": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.7.5",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.7.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "dependency-tree": "^7.1.0",
    "eslint": "^6.7.0",
    "fast-glob": "^2.2.7",
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.1",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "h-markdown-loader": "^0.1.3",
    "h-touch-emulator": "^0.1.1",
    "health-compiler": "0.0.1",
    "health-doc": "^0.1.9",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^9.4.3",
    "markdown-vetur": "0.0.5",
    "postcss": "^7.0.23",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "signale": "^1.4.0",
    "style-loader": "^1.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^2.3.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.7.2",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vue-template-es2015-compiler": "^1.9.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  }
}
